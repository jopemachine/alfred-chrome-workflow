{
  "author": {
    "name": "jopemachine",
    "email": "jopemachine@gmail.com",
    "url": "https://github.com/jopemachine/alfred-chrome-workflow"
  },
  "bugs": {
    "url": "https://github.com/jopemachine/alfred-chrome-workflow/issues"
  },
  "deprecated": false,
  "engines": {
    "node": ">=12"
  },
  "name": "alfred-chrome-workflow",
  "version": "0.4.4",
  "description": "Chrome kernel based browser workflow for Alfred 4",
  "scripts": {
    "test": "xo --fix",
    "lint": "xo --fix",
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jopemachine/alfred-chrome-workflow.git"
  },
  "keywords": [
    "alfred",
    "alfred-workflow",
    "alfred4-workflow",
    "alfy",
    "chrome-search",
    "chrome-download-log",
    "chrome-autofill-data",
    "chrome-media-history",
    "chrome-bookmark",
    "chrome-history",
    "edge",
    "chrome",
    "chromium",
    "chrome-canary",
    "brave"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jopemachine/alfred-chrome-workflow#readme",
  "dependencies": {
    "alfy": "^0.10.0",
    "better-sqlite3": "^7.5.1",
    "byte-size": "^7.0.1",
    "fs-extra": "^10.0.0",
    "humanize-duration": "^3.25.1",
    "lodash": "^4.17.21",
    "outdent": "^0.8.0",
    "path-exists": "^4.0.0",
    "psl": "^1.8.0"
  },
  "files": [
    "assets/*.png",
    "info.plist",
    "src/*.js",
    "icon.png",
    "conf.json",
    "*.md",
    "cache/empty"
  ],
  "devDependencies": {
    "xo": "^0.39.1"
  },
  "xo": {
    "rules": {
      "camelcase": 0,
      "unicorn/filename-case": 0,
      "import/no-unassigned-import": 0,
      "unicorn/prefer-string-slice": 0,
      "unicorn/no-process-exit": 0
    }
  }
}
